{
  "css.validate": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "debug.node.autoAttach": "disabled",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.fixAll.prettier": "explicit",
    "source.fixAll.markdownlint": "explicit",
    "source.organizeImports": "explicit",
    "source.sortMembers": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "'Fira Code Retina', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": true,
  "editor.stickyTabStops": true,
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "Comment",
        "scope": [
          "comment",
          "comment.block",
          "comment.block.documentation",
          "comment.line",
          "comment.line.double-slash",
          "punctuation.definition.comment"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      }
    ]
  },
  "editor.wordWrap": "on",
  "eslint.experimental.useFlatConfig": true,
  "extensions.autoCheckUpdates": false,
  "extensions.autoUpdate": false,
  "extensions.ignoreRecommendations": true,
  "files.autoSave": "onFocusChange",
  "git.confirmSync": false,
  "indentRainbow.indentSetter": {},
  "javascript.updateImportsOnFileMove.enabled": "always",
  "remote.autoForwardPorts": false,
  "remote.restoreForwardedPorts": false,
  "rust-analyzer.check.overrideCommand": [
    "cargo-clippy",
    "--all-targets",
    "--workspace",
    "--message-format=json"
  ],
  "scss.lint.unknownAtRules": "ignore",
  "security.workspace.trust.enabled": true,
  "svelte.enable-ts-plugin": true,
  "tailwindCSS.experimental.classRegex": ["class: \"(.*)\""],
  "tailwindCSS.includeLanguages": {
    "rust": "html"
  },
  "terminal.integrated.confirmOnExit": "always",
  "terminal.integrated.confirmOnKill": "always",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.enableMultiLinePasteWarning": true,
  "terminal.integrated.fontFamily": "MesloLGM Nerd Font",
  "terminal.integrated.shellIntegration.enabled": false,
  "terminal.integrated.suggest.enabled": true,
  "task.allowAutomaticTasks": "off",
  "todohighlight.include": [
    "**/*.css",
    "**/*.html",
    "**/*.js",
    "**/*.jsx",
    "**/*.php",
    "**/*.scss",
    "**/*.ts",
    "**/*.tsx"
  ],
  "todohighlight.keywords": [
    {
      "backgroundColor": "none",
      "color": "#ff375f",
      "text": "Hard"
    },
    {
      "backgroundColor": "none",
      "color": "#ffc01e",
      "text": "Medium"
    },
    {
      "backgroundColor": "none",
      "color": "#00b8a3",
      "text": "Easy"
    },
    {
      "backgroundColor": "#39C",
      "color": "#FFF",
      "text": "ASK:"
    },
    {
      "backgroundColor": "#C39",
      "color": "#FFF",
      "text": "DONE:"
    },
    {
      "backgroundColor": "#966",
      "color": "#FFF",
      "text": "MATERIALS:"
    },
    {
      "backgroundColor": "#FA0",
      "color": "#FFF",
      "text": "POINT:"
    },
    "REF:",
    "REVIEW:",
    {
      "backgroundColor": "#366",
      "color": "#FFF",
      "text": "TABLE:"
    },
    {
      "backgroundColor": "#393",
      "color": "#FFF",
      "text": "TEXTURE:"
    },
    {
      "backgroundColor": "#F36",
      "color": "#FFF",
      "text": "TRY:"
    },
    {
      "backgroundColor": "#C00",
      "color": "#FFF",
      "text": "WARNING:"
    }
  ],
  "triggerTaskOnSave.tasks": {
    "rustywind-fix": ["**/*.rs"]
  },
  "triggerTaskOnSave.on": true,
  "triggerTaskOnSave.showNotifications": true,
  "update.mode": "none",
  "update.showReleaseNotes": false,
  "workbench.activityBar.orientation": "vertical",
  "workbench.colorTheme": "Monokai Vibrant (Rust)",
  "workbench.editor.customLabels.patterns": {
    "**/Cargo.toml": "${dirname} (Cargo.toml)",
    "**/index.*": "${dirname} (index)",
    "**/layout.tsx": "${dirname} (layout)",
    "**/lib.*": "${dirname} (lib)",
    "**/mod.*": "${dirname} (mod)",
    "**/page.tsx": "${dirname} (page)"
  },
  "workbench.editor.empty.hint": "hidden",
  "workbench.editorAssociations": {
    "*.glb": "default"
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.settings.applyToAllProfiles": [],
  "[css,json,yaml,yml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[markdown]": {
    "editor.defaultFormatter": "DavidAnson.vscode-markdownlint",
    "editor.formatOnSave": true
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer",
    "editor.formatOnSave": true,
    "editor.tabSize": 4
  },
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml",
    "editor.formatOnSave": true
  },
  "[typst]": {
    "editor.defaultFormatter": "myriad-dreamin.tinymist"
  },
  "[dockercompose]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  }
}
